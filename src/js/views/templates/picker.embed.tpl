<form class="typely-form" autocomplete="off">
	<div class="form-group">
		<label for="code">Paste Embed Code</label>
	</div>
	<div class="form-group">
		<textarea name="code" rows="6" class="form-control" placeholder="Paste custom embed code"></textarea>
	</div>
	<div class="form-group">
		<input type="text" class="form-control" name="caption" placeholder="Add a short caption">
	</div>
	<button type="submit" class="typely-btn typely-btn-default js-insert">Insert</button>
	<button type="button" class="typely-btn typely-btn-link js-cancel">cancel</button>
</form>
