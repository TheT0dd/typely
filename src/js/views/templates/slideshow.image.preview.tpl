<div class="img-control move-img">
	<i class="fa fa-arrows"></i>
</div>

<div class="img-container">
	<div class="thumb">
		<img src="<%= src %>" id="<%= id %>" data-type="<%= type %>">
	</div>
	<div class="caption">
		<input class="form-control" placeholder="<%= captionPlaceholder %>">
	</div>
</div>

<div class="img-control delete-img js-delete">
	<i class="fa fa-trash-o"></i>
</div>
